  },
  "babel": {
    "presets": [
      "node6"
    ]
  },
  "engines": {
    "node": "6.4.0"
  },
  "dependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-polyfill": "6.16.0",
    "babel-preset-node6": "^11.0.0",
    "debug": "2.2.0",
    "dotenv": "^2.0.0",
    "form-data": "2.1.1",
    "he": "1.1.0",
    "irc": "0.5.0",
    "irc-colors": "1.3.0",
    "moment": "2.15.2",
    "moment-timezone": "0.5.7",
    "node-fetch": "1.6.3",
    "twitter": "1.4.0"
  },
  "devDependencies": {
    "babel-eslint": "6.1.2",
    "eslint": "3.9.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "mocha": "3.1.2"
  }
