    "irc-colors": "1.2.1",
    "moment": "2.14.1",
    "moment-timezone": "0.5.4",
    "node-fetch": "1.5.3",
    "twitter": "1.3.0"
  },
  "devDependencies": {
    "babel-eslint": "6.1.2",
